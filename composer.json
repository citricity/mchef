{
    "name": "citricity/mchef",
    "version": "1.0.0",
    "description": "A docker wrapper enabling Moodle containers to be built from simple recipes",
    "license": "GPL-3.0-only",
    "require": {
        "symfony/console": "^5.4",
        "splitbrain/php-cli": "^1.3",
        "twig/twig": "^3.0"
    },
    "config": {
        "platform": {
            "php": "8.2"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
